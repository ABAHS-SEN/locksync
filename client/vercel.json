{
    "version": 2,
    "builds": [
      {
        "src": "src/index.html",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      { "src": "/",
       "dest": "/"
     },
      { "src": "/register",
       "dest": "/"
     },
      { "src": "/login",
       "dest": "/"
     },
      { "src": "/share",
       "dest": "/"
     },
      { "src": "/dashborad",
       "dest": "/"
     },
      { "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  